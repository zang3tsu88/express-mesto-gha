{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": "airbnb-base",
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
      "no-underscore-dangle": ["error", { "allow": ["_id"]}],
      "quotes": ["error", "double", { "avoidEscape": true }],
      "no-console": "off",
      "no-unused-vars": "warn"
    }
}
